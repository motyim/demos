type User {
    id: ID!
    name: String!
    email: String
    password: String
}

type SigninPayload {
  token: String
  user: User
}

input AuthData {
    email: String!
    password: String!
}

extend type Mutation {
    createUser(name: String! ,authProvider: AuthData!): User
    signinUser(auth:AuthData):SigninPayload
}

